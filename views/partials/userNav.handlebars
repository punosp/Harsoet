
<!--Start header-search  area-->
<section class="header-search">
    <div class="container">
        <div class="row">
            <div class="col-md-12 col-sm-12 col-xs-12">
            <div class="search-form pull-right">
                    <form action="#">
                        <div class="search">
                            <input type="search" name="search" value="" placeholder="Search Something">
                            <button type="submit"><span class="flaticon-magnifying-glass"></span></button>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</section>
<!--End header-search  area-->

<!--Start header area-->
<header class="header-area">
    <div class="container">
        <div class="row">
            <div class="col-lg-3 col-md-12 col-sm-12 col-xs-12">
                <div class="logo">
                    <a href="/"><img src="/images/logo1.png" alt="Awesome Logo" style="witdth: 50px;height:50px"></a>
                </div>
            </div>
            <div class="col-lg-9 col-md-12 col-sm-12 col-xs-12">
                <ul class="contact-info">
                    <li>
                        <div class="icon-holder">
                            <span class="flaticon-signs"></span>
                        </div>
                        <div class="content">
                            <h5 style="word-wrap: break-word; max-width: 210px;">{{partialData.primaryAddress.address}}, <br>
                            {{partialData.primaryAddress.city}}, {{partialData.primaryAddress.pin}},
                          {{partialData.primaryAddress.country}}</h5>
                        </div>

                    </li>
                    <li>
                        <div class="icon-holder">
                            <span class="flaticon-clock"></span>
                        </div>
                        <div class="content">
                            <h5>{{partialData.primaryAddress.from}} - {{partialData.primaryAddress.to}} <br>Mon - Sun</h5>
                        </div>
                    </li>
                    <li class="pdleft">
                        <div class="icon-holder">
                            <span class="flaticon-phone-call"></span>
                        </div>
                        <div class="content">
                            <h5>{{partialData.primaryAddress.phone}}<br>Call us for enquiry</h5>
                        </div>
                    </li>
                    <li class="request">
                        <a class="request-quote hvr-sweep-to-bottom" href="/admin/login">Admin Login</a>
                    </li>
                </ul>
            </div>
        </div>
    </div>
</header>
<!--End header area-->

