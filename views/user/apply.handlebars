<!--Start contact form area-->
<section class="contact-form-area">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="contact-form">
                    <div class="sec-title two">
                        <h1>Send Us Your Queries</h1>
                        <span class="border"></span>
                    </div>
                    <form action="/query/post" class="contact-form" action="post">
                        <div class="row">
                            <div class="col-md-6">
                                <div class="input-field">
                                    <input type="text" placeholder="Your Name*" name="name">
                                </div>
                                <div class="input-field">
                                    <input type="text" placeholder="Email Address*" name="email">
                                </div>
                                <div class="input-field">
                                    <input type="text" placeholder="Phone" name="phone">
                                </div>
                                <div class="input-field">
                                    <input type="text" placeholder="City" name="city">
                                </div>
																<div class="input-field">
                                    <input type="text" placeholder="P.C.B.%" name="subject">
                                </div>
																<div class="input-field">
                                    <input type="text" placeholder="Passing Year" name="passingYear">
                                </div>

                            </div>
                            <div class="col-md-6">
															<div class="input-field">
																	<input type="text" placeholder="Country Opted" name="countryOpted">
															</div>
															<div class="input-field">
																	<input type="text" placeholder="Medical college opted" name="medicalCollegeOpted">
															</div>
															<div class="select-field">
																	<select class="selectpicker" name="passport">
																			<option>Passport Ready</option>
																			<option>Passport in Process</option>
																			<option>Not Applied Yet</option>
																	</select>
																	<br>
																	<br>
															</div>
                                <textarea placeholder="Message*" name="message"></textarea>
                                <button type="submit">Submit Now</button>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</section>
<!--End contact form area-->
